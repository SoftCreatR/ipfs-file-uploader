{
  "name": "ipfs-file-uploader",
  "version": "1.0.0",
  "description": "Website for uploading files into IPFS",
  "scripts": {
    "clean": "rimraf ./dist/*",
    "dev": "parcel src/index.html",
    "build": "rimraf ./dist/* && parcel build src/index.html src/error.html --public-url '/'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "rimraf ./dist/* && parcel build src/index.html src/error.html --public-url '/' && ipfs add -r --quieter dist"
  },
  "author": "Sascha Greuel <hello@1-2.dev>",
  "license": "ISC",
  "keywords": [
    "ipfs",
    "upload",
    "decentralized",
    "blockchain"
  ],
  "bugs": {
    "url": "https://github.com/SoftCreatR/ipfs-file-uploader/issues"
  },
  "homepage": "https://github.com/SoftCreatR/ipfs-file-uploader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftCreatR/ipfs-file-uploader.git"
  },
  "dependencies": {
    "ipfs-css": "^1.3.0",
    "ipfs-http-client": "^52.0.5",
    "tachyons": "^4.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@parcel/packager-raw-url": "2.0.0-rc.0",
    "@parcel/transformer-jsonld": "2.0.0-rc.0",
    "parcel": "^2.0.0-rc.0",
    "rimraf": "^3.0.2"
  }
}

{
  "name": "ipfs-file-uploader",
  "version": "1.0.0",
  "description": "Website for uploading files into IPFS",
  "scripts": {
    "clean": "rimraf './dist/*'",
    "dev": "parcel src/index.html",
    "build": "yarn clean && parcel build src/index.html src/error.html --public-url '/' --no-source-maps",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "yarn build && ipfs add -r --quieter dist"
  },
  "author": "Sascha Greuel <hello@1-2.dev>",
  "license": "ISC",
  "keywords": [
    "ipfs",
    "upload",
    "decentralized",
    "blockchain"
  ],
  "bugs": {
    "url": "https://github.com/SoftCreatR/ipfs-file-uploader/issues"
  },
  "homepage": "https://github.com/SoftCreatR/ipfs-file-uploader#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftCreatR/ipfs-file-uploader.git"
  },
  "dependencies": {
    "ipfs-css": "^1.3.0",
    "ipfs-http-client": "^53.0.1",
    "tachyons": "^4.12.0"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "devDependencies": {
    "@parcel/config-default": "^2.0.0",
    "@parcel/package-manager": "^2.0.0",
    "@parcel/packager-raw-url": "^2.0.0",
    "@parcel/packager-xml": "^2.0.0",
    "@parcel/plugin": "^2.0.0",
    "@parcel/service-worker": "^2.0.0",
    "@parcel/transformer-jsonld": "2.0.0",
    "@parcel/transformer-raw": "^2.0.0",
    "@parcel/transformer-sass": "^2.0.0",
    "autoprefixer": "^10.4.0",
    "parcel": "^2.0.0",
    "parcel-config-pwa-manifest": "^0.1.0",
    "postcss": "^8.3.11",
    "rimraf": "^3.0.2"
  },
  "pwaManifest": {
    "name": "IPFS File Uploader",
    "shortName": "IPFS Upload",
    "startURL": "./index.html",
    "theme": "#6acad1",
    "generateIconOptions": {
      "baseIcon": "./src/img/ipfs_logo_notext.svg",
      "sizes": [
        192,
        384,
        512
      ],
      "genFavicons": true,
      "purpose": [
        "maskable",
        "any"
      ]
    }
  }
}
